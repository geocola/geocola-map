{
  "name": "can-geo",
  "version": "0.3.3",
  "description": "Web mapping web components built using CanJS",
  "repository": "https://github.com/roemhiltg/can-geo",
  "main": "index.js",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs",
    "export": "node build.js",
    "lint": "eslint . --fix",
    "preversion": "npm run test",
    "postversion": "npm run export && npm run build && npm publish && git push origin master && git push --tags && npm run changelog",
    "changelog": "changelog can-geo all > CHANGELOG.md && git add . && git commit . -m changelog && git push origin master"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "changelog": "^1.0.7",
    "documentjs": "^0.4.4",
    "eslint": "^3.12.1",
    "npm-watch": "^0.1.6",
    "steal": "^1.0.7",
    "steal-css": "^1.1.0",
    "steal-less": "^1.0.1",
    "steal-qunit": "^1.0.0",
    "steal-stache": "^3.0.4",
    "steal-tools": "^1.0.1",
    "testee": "^0.3.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "can-component": "^3.0.5",
    "can-define": "^1.0.11",
    "can-event": "^3.0.1",
    "can-route": "^3.0.5",
    "can-stache": "^3.0.13",
    "can-util": "^3.1.1",
    "can-view-model": "^3.1.2",
    "font-awesome": "^4.6.x",
    "jquery": "^3.1.1",
    "openlayers": "^3.20.0",
    "spectre-canjs": "^0.17.2",
    "spectre.css": "^0.1.31"
  },
  "steal": {
    "plugins": [
      "steal-less",
      "steal-css",
      "steal-stache"
    ],
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    }
  }
}
