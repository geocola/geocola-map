{
  "name": "geocola-map",
  "version": "0.1.0",
  "description": "Mapping components used by geocola",
  "repository": "https://github.com/geocola/geocola-map",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "jshint .",
    "pretest": "npm run lint"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "documentjs": "^0.4.x",
    "jshint": "latest",
    "npm-watch": "^0.1.x",
    "steal-qunit": "^0.1.x",
    "steal-tools": "^0.16.x",
    "testee": "^0.2.x"
  },
  "dependencies": {
    "bootstrap": "^3.3.x",
    "can": "^2.3.x",
    "font-awesome": "^4.6.x",
    "geocola-crud": "git+https://github.com/geocola/geocola-crud.git",
    "geocola-ui": "git+https://github.com/geocola/geocola-ui.git",
    "jquery": "^2.2.x",
    "openlayers": "^3.16.x",
    "steal": "^0.16.x"
  },
  "main": "index",
  "system": {
    "bundleAssets": true,
    "bundle": [
      "index"
    ],
    "npmIgnore": [
      "openlayers",
      "documentjs",
      "jshint",
      "steal-tools",
      "testee"
    ],
    "paths": {
      "bootstrap": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    },
    "shim": {
      "jquery": {
        "exports": "jQuery"
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      }
    }
  }
}
