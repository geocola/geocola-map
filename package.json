{
  "name": "can-geo",
  "version": "0.2.0",
  "description": "Web mapping web components built using CanJS",
  "repository": "https://github.com/roemhiltg/can-geo",
  "main": "index.js",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "jshint .",
    "pretest": "npm run lint"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "documentjs": "^0.4.x",
    "eslint": "^3.10.1",
    "jshint": "latest",
    "npm-watch": "^0.1.6",
    "steal-qunit": "^0.1.x",
    "steal-tools": "^0.16.8",
    "testee": "^0.2.x"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "can-boot": "^0.1.0",
    "can-component": "^3.0.2",
    "can-crud": "latest",
    "can-define": "^1.0.4",
    "can-event": "^3.0.1",
    "can-getfeatureinfo": "^0.3.0",
    "can-route": "^3.0.4",
    "can-stache": "^3.0.8",
    "can-ui": "latest",
    "can-util": "^3.0.12",
    "can-view-model": "^3.1.0",
    "font-awesome": "^4.6.x",
    "jquery": "^3.1.1",
    "openlayers": "^3.19.1",
    "spectre-canjs": "^0.11.1",
    "spectre.css": "^0.1.27",
    "steal": "^0.16.42",
    "steal-stache": "^3.0.1"
  },
  "system": {
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    }
  }
}
