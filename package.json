{
  "name": "can-geo",
  "version": "0.3.4",
  "description": "Web mapping web components built using CanJS",
  "repository": "https://github.com/roemhildtg/can-geo",
  "main": "index.js",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs",
    "export": "node build.js",
    "lint": "eslint . --fix",
    "preversion": "npm run test",
    "postversion": "npm run export && npm run build && npm publish && git push origin master && git push --tags && npm run changelog",
    "changelog": "changelog can-geo all > CHANGELOG.md && git add . && git commit . -m changelog && git push origin master"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "changelog": "^1.0.7",
    "documentjs": "^0.4.4",
    "eslint": "^3.14.1",
    "npm-watch": "^0.1.8",
    "steal-css": "^1.2.0",
    "steal-less": "^1.1.0",
    "steal-qunit": "^1.0.0",
    "steal-stache": "^3.0.5",
    "steal-tools": "^1.1.1",
    "testee": "^0.3.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "can-component": "^3.0.5",
    "can-define": "^1.0.15",
    "can-event": "^3.0.2",
    "can-route": "^3.0.6",
    "can-stache": "^3.0.19",
    "can-util": "^3.2.2",
    "can-view-model": "^3.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "openlayers": "^3.20.1",
    "spectre-canjs": "^0.22.2",
    "spectre.css": "^0.1.32"
  },
  "steal": {
    "plugins": [
      "steal-less",
      "steal-css",
      "steal-stache"
    ],
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    }
  }
}
